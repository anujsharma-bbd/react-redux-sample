    "presets": [
      "react",
      "es2015"
    ]
  },
  "jest": {
    "testRegex": "/unit_test_cases/.*-test.jsx",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|svg)$": "<rootDir>/unit_test_cases/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/__snapshots__/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/app/**/*.{js,jsx}",
      "!**/app/*.js",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/app/assets/**",
      "!**/build/**"
    ]
  }
