  "description": "demotest City || On line Survey Application",
  "main": "index.js",
  "engines": {
    "node": "6.9.0"
  },
  "scripts": {
    "test": "jest -u --watchAll",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "build:test": "webpack --config webpack.config.js",
    "build:production": "webpack --config config/webpack.prod.js",
    "build:development": "webpack -w --config config/webpack.dev.js"
  },
  "keywords": [
    "test"
  ],
